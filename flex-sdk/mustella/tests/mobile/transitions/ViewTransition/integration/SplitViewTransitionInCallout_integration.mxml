         <!-- pushView(), pushToFirstView(), popView(), popAll(), replaceView() tests inside Callout in SplitViewNavigator -->
		 <TestCase testID="SplitViewNavTransitionInCallout_slide_default" keywords="[SplitViewNavigator, ViewTransiton, Callout]" description="Default SlideViewTransition inside callout">
			 <setup>
                <ResetComponent target="svn" className="comps.TestNav" waitEvent="updateComplete" />
			 </setup>
			    <body>
                 <RunCode code="FlexGlobals.topLevelApplication.svn.showFirstViewNavigatorInPopUp(FlexGlobals.topLevelApplication)" />
                 <AssertPropertyValue target="svn.viewNavigatorPopUp" propertyName="isOpen" value="true" />
               <RunCode code="application.svn.snav1.pushView(views.NextView, null, null, application.svn.snav1.activeView.transitionSlide)" waitEvent="transitionStart" waitTarget="svn.snav1"/>
			 <AssertEvent target="svn.snav1" eventName="transitionEnd" eventClass="mx.events::FlexEvent"/>
            	 <AssertPropertyValue target="svn.snav1.actionBar" propertyName="title" value="NextView" />
			 </body>
		 </TestCase>
