    <testCases>
        
		<TestCase testID="ViewTransition_Integration_slide">
			<setup>
				<ResetComponent target="navigator" className="comps.QANavigator" waitEvent="viewChangeComplete" />
				<SetProperty target="navigator" propertyName="requestSeek" value="false" /> 
				<!--<SetProperty target="navigator" propertyName="requestSeekTime" value="100" />-->
			</setup>
			<body>
				<AssertMethodValue method="application.navigator.pushView(views.NextView, null, null, application.navigator.activeView.transitionSlide)" />
				<!--				 <WaitForEvent target="navigator" eventName="viewTransitionAssertionReady" />
				-->				 
				<AssertEvent target="navigator" eventName="transitionStart" eventClass="mx.events::FlexEvent"/>
				<AssertEvent target="navigator" eventName="transitionEnd" eventClass="mx.events::FlexEvent"/>
				<AssertPropertyValue target="navigator.actionBar" propertyName="title" value="NextView" />
			</body>
		</TestCase>
