	<!-- this set of lines form a template that must be in each unit test -->
	
	<fx:Script>
		<![CDATA[
			
			import assets.DSConstants;
			import spark.components.calendarClasses.DateSelectorDisplayMode;
			import views.InvalidDateView;				
			
			public static function init(o:DisplayObject):void
			{
			}
		]]>
	</fx:Script>
	
	<fx:Metadata>
		<![CDATA[
		    [Mixin]
		]]>
	</fx:Metadata>  
	
	<!-- end of set of lines that must be in each unit test  -->
	<testCases>
		
		<TestCase testID="DateSpinner_invalidDate_test1" keywords="[DateSpinner, property, invalidDate]" 
				  description="Test invalidDate is working for DateSpinner component">
			<setup>
				<ResetComponent target="navigator" className="comps.QANavigator" waitEvent="viewChangeComplete" />
				
				<RunCode code="application.navigator.pushView(views.InvalidDateView)" 
						 waitTarget="navigator" waitEvent="viewChangeComplete" timeout="18000" />
				
				<SetProperty target="navigator.activeView.dsD.mx_internal:dateTimeFormatterEx"
							 propertyName="dateTimePattern" value="MMMM d, yyyy h:mm:ss a z" />
				<RunCode code="application.navigator.activeView.dsD.mx_internal::dateTimeFormatterEx.styleChanged(null)" />
				<RunCode code="application.navigator.activeView.dsD.mx_internal::refreshDateTimeFormatter = true" />
			</setup>
			<body>
				<SetProperty target="navigator.activeView.dsD" propertyName="minDate" 
							 valueExpression="value=new Date(2000, 0, 1, 1, 1, 1)"
							 waitEvent="updateComplete"
							 />
				<SetProperty target="navigator.activeView.dsD" propertyName="maxDate" 
							 valueExpression="value=new Date(2099, 11, 31, 1, 1, 1)"
							 waitEvent="updateComplete"
							 />
				<SetProperty target="navigator.activeView.dsD" propertyName="selectedDate" 
							 valueExpression="value=new Date(2002, 5, 30, 1, 1, 1)"
							 waitEvent="updateComplete"
							 />
				<SetProperty target="navigator.activeView.dsD" propertyName="displayMode" 
							 valueExpression="value=DateSelectorDisplayMode.DATE"
							 waitEvent="updateComplete"
							 />
				<WaitForLayoutManager />
				<CompareBitmap url="../properties/baselines" target="navigator.activeView.dsD">
					<ConditionalValue deviceDensity="160" os="win" />
					<ConditionalValue deviceDensity="240" os="android" />
					<ConditionalValue deviceDensity="320" os="win" />
					<ConditionalValue deviceDensity="320" os="ios" />
					<ConditionalValue deviceDensity="160" os="qnx" />
				</CompareBitmap>
			</body>
		</TestCase>
        
