		<TestCase testID="DS_zh_CN_selectedDate_4" 
				  keywords="[DateSpinner, property, selectedDate, set, get]" 
				  description="Test the set get function of property selectedDate for DateSpinner component works well.">
			<setup>
				<ResetComponent target="navigator" 
								className="comps.QANavigator" waitEvent="viewChangeComplete" />
				<RunCode code="application.navigator.pushView(views.zh_CNView)" 
						 waitTarget="navigator" waitEvent="viewChangeComplete" timeout="18000" />
						 
				<SetProperty target="navigator.activeView.ds_main.mx_internal:dateTimeFormatterEx"
                             propertyName="dateTimePattern" value="y年M月d日 ah时mm分ss秒z" />
               <RunCode code="application.navigator.activeView.ds_main.mx_internal::dateTimeFormatterEx.styleChanged(null)" />
                <RunCode code="application.navigator.activeView.ds_main.mx_internal::refreshDateTimeFormatter = true" />
				
				<SetProperty target="navigator.activeView.ds_main" propertyName="displayMode" 
							 valueExpression="value=DateSelectorDisplayMode.DATE"
							 />
				<SetProperty target="navigator.activeView.ds_main" propertyName="selectedDate" 
							 valueExpression="value=new Date(1997, 4, 23, 3, 18, 55)"
							 waitEvent="updateComplete"
							 />
			</setup>
			<body>
				<AssertPropertyValue target="navigator.activeView.ds_main" propertyName="displayMode" 
									 valueExpression="value=DateSelectorDisplayMode.DATE"/>
				<AssertPropertyValue target="navigator.activeView.ds_main" propertyName="selectedDate" 
									 valueExpression="value=new Date(1997, 4, 23, 3, 18, 55)"/>
				
				<AssertMethodValue method="value=application.navigator.activeView.ds_main.getYearRenderer(0).label" 
								   value="1997年" />
				<AssertMethodValue method="value=application.navigator.activeView.ds_main.getMonthRenderer(0).label" 
								   value="五月">
					<ConditionalValue value="五月" os="win"/>
					<ConditionalValue value="五月" os="android"/>
					<ConditionalValue value="五月" os="ios"/>
				</AssertMethodValue>
				<AssertMethodValue method="value=application.navigator.activeView.ds_main.getDateRenderer(0).label" 
								   value="23日" />
				
				<SetProperty target="navigator.activeView.ds_main" propertyName="selectedDate" 
							 valueExpression="value=new Date(1999, 11, 31)"
							 waitEvent="updateComplete"
							 />
				<AssertPropertyValue target="navigator.activeView.ds_main" propertyName="selectedDate" 
									 valueExpression="value=new Date(1999, 11, 31)"/>
				
				<AssertMethodValue method="value=application.navigator.activeView.ds_main.getYearRenderer(0).label" 
								   value="1999年" />
				<AssertMethodValue method="value=application.navigator.activeView.ds_main.getMonthRenderer(0).label" 
								   value="十二月">
					<ConditionalValue value="十二月" os="win"/>
					<ConditionalValue value="十二月" os="android"/>
					<ConditionalValue value="十二月" os="ios"/>
				</AssertMethodValue>
				<AssertMethodValue method="value=application.navigator.activeView.ds_main.getDateRenderer(0).label" 
								   value="31日" />
			</body>
		</TestCase>
		
