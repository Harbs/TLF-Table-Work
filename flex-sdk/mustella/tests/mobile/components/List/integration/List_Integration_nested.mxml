    <testCases>
        
        <!--
        ========================
        Testing Nested Scrolling 
        ========================
        
        Check the scroll positions and selection of nested Scroller + List.
        
        BasicLayout nested inside VerticalLayout
        
        + scroll down inside the nested basic layout should scroll only the basic layout
        + scroll right inside the nested basic layout should scroll only the basic layout
        + scroll diagonally down+right inside the nested basic layout should scroll only the basic layout
        
        VerticalLayout nested inside VerticalLayout
        
        + scroll down inside the nested vertical list should scroll only the vertical list
        + scroll right inside the nested vertical list should get selected since outer has nowhere to scroll
        + scroll diagonally down+right inside the nested vertical list should scroll only the vertical list
        
        HorizontalLayout nested inside VerticalLayout
        
        + scroll down inside the nested horizontal list should scroll the outer scroller
        + scroll right inside the nested horizontal list should only scroll the horizontal list
        + scroll diagonally down+right (favoring minor axis) inside the nested horizontal list should scroll the horizontal list
        + scroll diagonally down+right (exactly 45 degrees) inside the nested horizontal list should scroll the horizontal list
        + scroll diagonally down+right (favoring major axis) inside the nested horizontal list should scroll the outer scroller
        
        BasicLayout nested inside HorizontalLayout
        
        + scroll down inside the nested basic layout should scroll only the basic layout
        + scroll right inside the nested basic layout should scroll only the basic layout
        + scroll diagonally down+right inside the nested basic layout should scroll only the basic layout
        
        VerticalLayout nested inside HorizontalLayout
        
        + scroll down inside the nested vertical list only scroll the vertical list
        + scroll right inside the nested vertical list should scroll the outer scroller
        + scroll diagonally down+right (favoring major axis) inside the nested vertical list should scroll the outer scroller
        + scroll diagonally down+right (exactly 45 degrees) inside the nested vertical list should scroll the vertical list
        + scroll diagonally down+right (favoring minor axis) inside the nested vertical list should scroll the vertical list
        
        HorizontalLayout nested inside HorizontalLayout
        
        + scroll down inside the nested horizontal list should get selected since outer has nowhere to scroll 
        + scroll right inside the nested horizontal list should scroll only the horizontal list
        + scroll diagonally down+right inside the nested horizontal list should scroll only the inner horizontal list
                
        BasicLayout nested inside BasicLayout
        
        + scroll down inside the nested basic layout should scroll only the inner basic layout
        + scroll right inside the nested basic layout should scroll only the inner basic layout
        + scroll diagonally down+right inside the nested basic layout should scroll only the inner basic layout
        
        VerticalLayout nested inside BasicLayout
        
        + scroll down inside the nested vertical list should scroll only the vertical list
        + scroll right inside the nested vertical list should scroll the outer scroller
        + scroll diagonally down+right (favoring minor axis) inside the nested vertical list should scroll the vertical list
        + scroll diagonally down+right (exactly 45 degrees) inside the nested vertical list should scroll the outer scroller
        + scroll diagonally down+right (favoring major axis) inside the nested vertical list should scroll the outer scroller
        
        HorizontalLayout nested inside BasicLayout
        
        + scroll down inside the nested horizontal list should scroll only scroll the outer scroller
        + scroll right inside the nested horizontal list should scroll only scroll the horizontal list
        + scroll diagonally down+right (favoring major axis) inside the nested horizontal list should scroll the outer scroller
        + scroll diagonally down+right (exactly 45 degrees) inside the nested horizontal list should scroll the horizontal list
        + scroll diagonally down+right (favoring minor axis) inside the nested horizontal list should scroll the hlist
        
        -->
        
        <TestCase testID="List_Integration_nested_outer" keywords="[List, Integration]" description="sanity check: scroll the outer without affecting the inners">
            <setup>
                <ResetComponent target="navigator" className="comps.QANavigator" waitEvent="viewChangeComplete" />
            </setup>
            <body>
                <RunCode code="application.navigator.pushView(views.NestedInVerticalView)" waitTarget="navigator" waitEvent="viewChangeComplete" timeout="6000" />
                
                <SimulateMouseGesture target="navigator.activeView.outerScroller" dragYFrom="75" dragYTo="25" dragXFrom="5" dragXTo="5" />
                <AssertPropertyValue target="navigator.activeView.outerScroller.viewport" propertyName="verticalScrollPosition" valueExpression="value=(50 - List.getSlopValue())" />
                <AssertPropertyValue target="navigator.activeView.outerScroller.viewport" propertyName="horizontalScrollPosition" value="0" />
                <AssertPropertyValue target="navigator.activeView.innerScroller.viewport" propertyName="verticalScrollPosition" value="0" />
                <AssertPropertyValue target="navigator.activeView.innerScroller.viewport" propertyName="horizontalScrollPosition" value="0" />
                <AssertPropertyValue target="navigator.activeView.vList.dataGroup" propertyName="verticalScrollPosition" value="0" />
                <AssertPropertyValue target="navigator.activeView.vList.dataGroup" propertyName="horizontalScrollPosition" value="0" />
                <AssertPropertyValue target="navigator.activeView.hList.dataGroup" propertyName="verticalScrollPosition" value="0" />
                <AssertPropertyValue target="navigator.activeView.hList.dataGroup" propertyName="horizontalScrollPosition" value="0" />
                <AssertPropertyValue target="navigator.activeView.vList.selectedItems" propertyName="length" value="0" />
                <AssertPropertyValue target="navigator.activeView.hList.selectedItems" propertyName="length" value="0" />
            </body>
        </TestCase>
        
