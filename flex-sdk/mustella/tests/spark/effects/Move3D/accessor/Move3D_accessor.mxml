<UnitTester
    testSWF="../SWFs/Move3D_main.mxml"
    testDir="spark/effects/Move3D/accessor/"
    xmlns:mx="http://www.adobe.com/2006/mxml"
    xmlns="*">

    <mx:Script>
        <![CDATA[
        import spark.effects.Move3D;
        
        public static function init(o:DisplayObject):void{}
        ]]>
    </mx:Script>
    <mx:Metadata><![CDATA[[Mixin]]]></mx:Metadata>
    <testCases>

        <!-- filter -->
        <TestCase testID="Move3D_filter_Default" keywords="[Move3D, accessor, filter]" description="Test the default value of the filter property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).filter" value="null"/>
            </body>
        </TestCase>
        <!-- customFilter -->
        <TestCase testID="Move3D_customFilter_Default" keywords="[Move3D, accessor, customFilter]" description="Test the default value of the customFilter property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).customFilter" value="null"/>
            </body>
        </TestCase>
        <!-- effectTargetHost -->
        <TestCase testID="Move3D_effectTargetHost_Default" keywords="[Move3D, accessor, effectTargetHost]" description="Test the default value of the effectTargetHost property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).effectTargetHost" value="null"/>
            </body>
        </TestCase>
        <!-- target -->
        <TestCase testID="Move3D_target_Default" keywords="[Move3D, accessor, target]" description="Test the default value of the target property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).target" value="null"/>
            </body>
        </TestCase>
        <!-- perElementOffset -->
        <TestCase testID="Move3D_perElementOffset_Default" keywords="[Move3D, accessor, perElementOffset]" description="Test the default value of the perElementOffset property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).perElementOffset" value="0"/>
            </body>
        </TestCase>
        <!-- isPlaying -->
        <TestCase testID="Move3D_isPlaying_Default" keywords="[Move3D, accessor, isPlaying]" description="Test the default value of the isPlaying property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).isPlaying" value="false"/>
            </body>
        </TestCase>
        <!-- playheadTime -->
        <TestCase testID="Move3D_playheadTime_Default" keywords="[Move3D, accessor, playheadTime]" description="Test the default value of the playheadTime property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).playheadTime" value="0"/>
            </body>
        </TestCase>
        <!-- relevantProperties -->
        <TestCase testID="Move3D_relevantProperties_Default" keywords="[Move3D, accessor, relevantProperties]" description="Test the default value of the relevantProperties property.">
            <body>
                <AssertMethodValue 
					method="value=(new Move3D()).relevantProperties" 
					value="translationX,translationY,translationZ,postLayoutTranslationX,postLayoutTranslationY,postLayoutTranslationZ,left,right,top,bottom,horizontalCenter,verticalCenter,baseline,width,height"/>
            </body>
        </TestCase>
        <!-- targets -->
        <TestCase testID="Move3D_targets_Default" keywords="[Move3D, accessor, targets]" description="Test the default value of the targets property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).targets" value=""/>
            </body>
        </TestCase>
        <!-- duration -->
        <TestCase testID="Move3D_duration_Default" keywords="[Move3D, accessor, duration]" description="Test the default value of the duration property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).duration" value="500"/>
            </body>
        </TestCase>
        <!-- hideFocusRing -->
        <TestCase testID="Move3D_hideFocusRing_Default" keywords="[Move3D, accessor, hideFocusRing]" description="Test the default value of the hideFocusRing property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).hideFocusRing" value="false"/>
            </body>
        </TestCase>
        <!-- className -->
        <TestCase testID="Move3D_className_Default" keywords="[Move3D, accessor, className]" description="Test the default value of the className property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).className" value="Move3D"/>
            </body>
        </TestCase>
        <!-- triggerEvent -->
        <TestCase testID="Move3D_triggerEvent_Default" keywords="[Move3D, accessor, triggerEvent]" description="Test the default value of the triggerEvent property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).triggerEvent" value="null"/>
            </body>
        </TestCase>
        <!-- relevantStyles -->
        <TestCase testID="Move3D_relevantStyles_Default" keywords="[Move3D, accessor, relevantStyles]" description="Test the default value of the relevantStyles property.">
            <body>
                <AssertMethodValue method="value=(new Move3D()).relevantStyles" value="left,right,top,bottom,horizontalCenter,verticalCenter,baseline"/>
            </body>
        </TestCase>
        
    </testCases>
</UnitTester>

