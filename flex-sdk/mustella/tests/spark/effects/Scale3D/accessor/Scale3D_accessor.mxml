<UnitTester
    testSWF="../SWFs/Scale3D_main.mxml"
    testDir="spark/effects/Scale3D/accessor/"
    xmlns:mx="http://www.adobe.com/2006/mxml"
    xmlns="*">

    <mx:Script>
        <![CDATA[
        import spark.effects.Scale3D;
        
        public static function init(o:DisplayObject):void{}
        ]]>
    </mx:Script>
    <mx:Metadata><![CDATA[[Mixin]]]></mx:Metadata>
    <testCases>

        <!-- filter -->
        <TestCase testID="Scale3D_filter_Default" keywords="[Scale3D, accessor, filter]" description="Test the default value of the filter property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).filter" value="null"/>
            </body>
        </TestCase>
        <!-- customFilter -->
        <TestCase testID="Scale3D_customFilter_Default" keywords="[Scale3D, accessor, customFilter]" description="Test the default value of the customFilter property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).customFilter" value="null"/>
            </body>
        </TestCase>
        <!-- effectTargetHost -->
        <TestCase testID="Scale3D_effectTargetHost_Default" keywords="[Scale3D, accessor, effectTargetHost]" description="Test the default value of the effectTargetHost property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).effectTargetHost" value="null"/>
            </body>
        </TestCase>
        <!-- target -->
        <TestCase testID="Scale3D_target_Default" keywords="[Scale3D, accessor, target]" description="Test the default value of the target property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).target" value="null"/>
            </body>
        </TestCase>
        <!-- perElementOffset -->
        <TestCase testID="Scale3D_perElementOffset_Default" keywords="[Scale3D, accessor, perElementOffset]" description="Test the default value of the perElementOffset property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).perElementOffset" value="0"/>
            </body>
        </TestCase>
        <!-- isPlaying -->
        <TestCase testID="Scale3D_isPlaying_Default" keywords="[Scale3D, accessor, isPlaying]" description="Test the default value of the isPlaying property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).isPlaying" value="false"/>
            </body>
        </TestCase>
        <!-- playheadTime -->
        <TestCase testID="Scale3D_playheadTime_Default" keywords="[Scale3D, accessor, playheadTime]" description="Test the default value of the playheadTime property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).playheadTime" value="0"/>
            </body>
        </TestCase>
        <!-- relevantProperties -->
        <TestCase testID="Scale3D_relevantProperties_Default" keywords="[Scale3D, accessor, relevantProperties]" description="Test the default value of the relevantProperties property.">
            <body>
                <AssertMethodValue 
					method="value=(new Scale3D()).relevantProperties" 
					value="scaleX,scaleY,scaleZ,postLayoutScaleX,postLayoutScaleY,postLayoutScaleZ,width,height"/>
            </body>
        </TestCase>
        <!-- targets -->
        <TestCase testID="Scale3D_targets_Default" keywords="[Scale3D, accessor, targets]" description="Test the default value of the targets property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).targets" value=""/>
            </body>
        </TestCase>
        <!-- duration -->
        <TestCase testID="Scale3D_duration_Default" keywords="[Scale3D, accessor, duration]" description="Test the default value of the duration property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).duration" value="500"/>
            </body>
        </TestCase>
        <!-- hideFocusRing -->
        <TestCase testID="Scale3D_hideFocusRing_Default" keywords="[Scale3D, accessor, hideFocusRing]" description="Test the default value of the hideFocusRing property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).hideFocusRing" value="false"/>
            </body>
        </TestCase>
        <!-- className -->
        <TestCase testID="Scale3D_className_Default" keywords="[Scale3D, accessor, className]" description="Test the default value of the className property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).className" value="Scale3D"/>
            </body>
        </TestCase>
        <!-- triggerEvent -->
        <TestCase testID="Scale3D_triggerEvent_Default" keywords="[Scale3D, accessor, triggerEvent]" description="Test the default value of the triggerEvent property.">
            <body>
                <AssertMethodValue method="value=(new Scale3D()).triggerEvent" value="null"/>
            </body>
        </TestCase>
        <!-- relevantStyles -->
        <TestCase testID="Scale3D_relevantStyles_Default" keywords="[Scale3D, accessor, relevantStyles]" description="Test the default value of the relevantStyles property.">
            <body>
                <AssertMethodValue 
					method="value=(new Scale3D()).relevantStyles" 
					value=""/>
            </body>
        </TestCase>
        
    </testCases>
</UnitTester>

