<!-- Warning: Both Illustrator CS4 and FXG Viewer render stroke with alpha=0.5 with a stroke two different color combination. Why? -->
<!-- FXG 1.0 Test Stroke a path with solid color with opacity.                   -->
<!-- The SolidColor element fills a path or shape with a single solid color or solid color with opacity.	-->
<!-- Author : Min Plunkett  26 Feb-2009                       			-->
<!--                                                                       				-->
<!-- History:                                                              				-->
<!--  26-Feb-2009, created.                                         			-->
<!-- ==========================================	-->
<!-- ==========================================	-->
<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">
	<Ellipse x="10" y="10" width="200" height="100">
		<fill>
			<SolidColor alpha="0.5" color="#0000FF"/>
		</fill>
		<stroke>
			<SolidColorStroke color="#FF0000" alpha="1" weight="10"/>
		</stroke>
	</Ellipse>
	<Ellipse x="10" y="140" width="200" height="100">
		<fill>
			<SolidColor alpha="0.5" color="#0000FF"/>
		</fill>
		<stroke>
			<SolidColorStroke color="#FF0000" alpha="0.5" weight="10"/>
		</stroke>
	</Ellipse>
	<Ellipse x="10" y="270" width="200" height="100">
		<fill>
			<SolidColor alpha="0.5" color="#0000FF"/>
		</fill>
		<stroke>
			<SolidColorStroke color="#FF0000" alpha="0" weight="10"/>
		</stroke>
	</Ellipse>
</Graphic>