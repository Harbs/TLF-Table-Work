<!--                                                                                  		-->
<!-- FXG 1.0 Tests of vibility                              -->
<!-- FXG elements can control whether or not they are visible by specifying a visible attribute, which defaults to true. The visibility of a graphical element is defined by the visible attribute of the element and all of its parent elements. That is, if any parent of the element is hidden, that element is not rendered. Non-visible graphical elements still exist and are considered for any bounding box calculations necessary to render the document.
-->
<!--                                                                       				-->
<!-- Author : Min Plunkett 26 Feb-2009                       			-->
<!-- History:                                                              				-->
<!--  26-Feb-2009, created.                                         			-->
<!-- ==========================================	-->
<!-- ==========================================	-->
<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">
	<Group blendMode="normal" alpha="0.5">
		<Rect x="100" y="100" width="100" height="100" visible="true">
			<fill>
				<SolidColor color="#FF0000"/>
			</fill>
		</Rect>
		<Rect x="0" y="0" width="200" height="200" visible="false">
			<fill>
				<SolidColor color="#0000FF"/>
			</fill>
		</Rect>
	</Group>
	<Group blendMode="normal" alpha="1" visible="true">
		<Rect x="125" y="131" width="393" height="167">
			<fill>
				<LinearGradient x = "152" y = "95" scaleX = "274" rotation = "-17">
					<GradientEntry color="#203c00" ratio="0" alpha="1"/>
					<GradientEntry color="#1b971b" ratio="0.28" alpha="1"/>
					<GradientEntry color="#1e4f45" ratio="0.45" alpha="1"/>
					<GradientEntry color="#6aec01" ratio="0.6" alpha="1"/>
					<GradientEntry color="#1b971b" ratio="0.75" alpha="1"/>
					<GradientEntry color="#203c00" ratio="1" alpha="1"/>
				</LinearGradient>
			</fill>
			<stroke>
				<SolidColorStroke color="#cc3300" weight="1"/>
			</stroke>
		</Rect>	
	</Group>
	<Group blendMode="normal" visible="false">
		<Rect x="200" y="200" width="200" height="200">
			<fill>
				<SolidColor color="#FF0000"/>
			</fill>
		</Rect>
	</Group>
</Graphic>