<!--                                                                                  		       -->
<!-- FXG 1.0 Test ability to handle coordinate space transformation matrix with the 'ellipse' basic-shapes element. 
 The top group comprises of  7 different ellipses and the bottom group comrpises of the same ellipse elements with the exception that 
different transforms have been applied to each of those elements.-->
<!-- Author : Vivek Agrawal  3 March-2009                       			-->
<!--                                                                       				-->
<!-- History:                                                              				-->
<!--  3 March-2009 , created.                                         			-->
<!-- ==========================================	-->
<!-- ==========================================	-->
<Graphic version="1.0" viewHeight="359" viewWidth="479" xmlns="http://ns.adobe.com/fxg/2008" >

<Group x="18" y="14">
	<Ellipse width="60" height="100">
		<stroke>
			<SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
		</stroke>
		<transform>
			<Transform>
				<matrix>
					<Matrix tx="100.0" ty="99.0" a="2" b="4" c="2" d="2"/>
				</matrix>
			</Transform>
        </transform>
	</Ellipse>
	<Ellipse  width="60" height="100">
		<fill>
			<SolidColor color="#ff0000"/>
		</fill>
		<transform>
			<Transform>
				<matrix>
					<Matrix tx="50.0" ty="99.0" a="2" b="0" c="0" d="1"/>
				</matrix>
			</Transform>
        </transform>		
	</Ellipse>
	<Ellipse  width="70" height="70">
		<stroke>
			<SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
		</stroke>
		<transform>
			<Transform>
				<matrix>
					<Matrix tx="30.0" ty="99.0" a="1" b="0" c="0" d="1"/>
				</matrix>
			</Transform>
        </transform>		
	</Ellipse>
	<Ellipse  width="70" height="70">
		<fill>
			<SolidColor color="#ffff00"/>
		</fill>
		<transform>
			<Transform>
				<matrix>
					<Matrix tx="10.0" ty="99.0" a="1" b="0" c="0" d="1"/>
				</matrix>
			</Transform>
        </transform>		
	</Ellipse>
	<Ellipse  width="60" height="100">
		<stroke>
			<SolidColorStroke color="#0000ff" caps="none" weight="8" joints="miter" miterLimit="4"/>
		</stroke>
		<transform>
			<Transform>
				<matrix>
					<Matrix tx="100.0" ty="200.0" a="1" b="0" c="0" d="1"/>
				</matrix>
			</Transform>
        </transform>		
	</Ellipse>
	<Ellipse  width="60" height="100">
		<fill>
			<SolidColor color="#00ff00"/>
		</fill>
		<stroke>
			<SolidColorStroke color="#0000ff" caps="none" weight="8" joints="miter" miterLimit="4"/>
		</stroke>
		<transform>
			<Transform>
				<matrix>
					<Matrix tx="190.0" ty="200.0" a="1" b="0" c="0" d="1"/>
				</matrix>
			</Transform>
        </transform>
	</Ellipse>
	<Ellipse  width="140" height="80">
		<fill>
			<SolidColor color="#00ff00"/>
		</fill>
		<stroke>
			<SolidColorStroke color="#0000ff" caps="none" weight="8" joints="miter" miterLimit="4"/>
		</stroke>
		<transform>
			<Transform>
				<matrix>
					<Matrix tx="300.0" ty="200.0" a="1" b="0" c="0" d="1"/>
				</matrix>
			</Transform>
        </transform>		
	</Ellipse>
</Group>
</Graphic>
