<!--                                                                                  		-->
<!-- FXG 1.0 Tests scaling grid applied in a group containing 9 ellipses laid out in a matrix   -->
<!--     															-->
<!--                                                                             			-->
<!-- Author : Peter Farland 15 March-2009                       			                  -->
<!--                                                                       				-->
<!-- History:                                                              				-->
<!--  15-March-2009, created.                                         			            -->
<!-- ==========================================	-->
<!-- ==========================================	-->

<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">
    <Group x="10" y="10" scaleX="2" scaleY="2"
        scaleGridLeft="25" scaleGridTop="25" scaleGridRight="50" scaleGridBottom="50">

        <!-- This invisible Rect pads out the bounds of the Group so that the
             9-slice scaling grid falls wholly within the bounds. -->
        <Rect x="0" y="0" width="75" height="75">
            <fill>
                <SolidColor color="#FFFFFF" alpha="0" />
            </fill>
        </Rect>

        <Ellipse x="0" y="0" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="25" y="0" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="50" y="0" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="0" y="25" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="25" y="25" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="50" y="25" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="0" y="50" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="25" y="50" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
        <Ellipse x="50" y="50" width="25" height="25">
            <stroke>
                <SolidColorStroke color="#000000" weight="1"/>
            </stroke>
            <fill>
                <SolidColor color="#FF9900" />
            </fill>
        </Ellipse>
    </Group>
</Graphic>
