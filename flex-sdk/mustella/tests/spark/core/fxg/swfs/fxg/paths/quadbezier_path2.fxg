<!-- FXG 1.0 Tests of Quadratic bezier curves drawn with commands, M, m, Q. q. T. t. Z, z      		  	-->
<!-- Author : Vivek Agrawal  12 Feb-2009
		Min Plunkett  13 Mar-2009                       										-->
<!--                                                                       											-->
<!-- History:                                                              											-->
<!--  12 Feb-2009, created by Vivek Agrawal. -->
<!--  13 Mar-2009, modified by Min Plunkett. Added rule called out in the FXG spec:Draws a quadratic Bezier curve when there is no previous command or if the previous command was not a Q, q, T or t.        -->
<!-- ==========================================								-->
<!-- ==========================================								-->
<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">
<!--======================================================================-->
<!--Content of Test Case follows...                                                      =====================-->
<!--======================================================================-->
	<Group x="50" y="50">
	 
	      <TextGraphic fontFamily="MyVeraSpark" fontWeight="bold" lineHeight="120%" whiteSpaceCollapse="preserve" kerning="on" x="0" y="500">
		<content><p><span>Draws a quadratic Bezier curve when there is no previous command or </span></p>
		<p><span>if the previous command was not a Q, q, T or t, assume the control point is </span></p>
		<p><span>coincident with the current point.</span></p>
		<p><span color="#ff0000">Problem rendering.</span></p></content>
	      </TextGraphic>
	      <Group x="0" y="550">
		      <Path winding="nonZero"  data="M  240  96   t   47 0  t  47 0  t   47 0  t   47 0 z" >
			<stroke>
			  <SolidColorStroke color="#ff0000" caps="none" weight="1" joints="miter" miterLimit="4"/>
			</stroke>
		      </Path>
		      <TextGraphic fontFamily="MyVeraSpark" lineHeight="120%" whiteSpaceCollapse="preserve" kerning="on" x="380" y="36">
			<content><p><span>M, q, t, t, t, t, z</span></p></content>
		      </TextGraphic>
		      <Rect x="238" y="94" width="4" height="4">
			<fill>
			  <SolidColor color="#0000ff"/>
			</fill>
		      </Rect>
		      <Rect x="285" y="94" width="4" height="4">
			<fill>
			  <SolidColor color="#0000ff"/>
			</fill>
		      </Rect>
		      <Rect x="332" y="94" width="4" height="4">
			<fill>
			  <SolidColor color="#0000ff"/>
			</fill>
		      </Rect>
		      <Rect x="379" y="94" width="4" height="4">
			<fill>
			  <SolidColor color="#0000ff"/>
			</fill>
		      </Rect>
		      <Rect x="426" y="94" width="4" height="4">
			<fill>
			  <SolidColor color="#0000ff"/>
			</fill>
		      </Rect>
		      <Rect x="473" y="94" width="4" height="4">
			<fill>
			  <SolidColor color="#0000ff"/>
			</fill>
		      </Rect>	      
	      </Group>
	      
	</Group>	      
</Graphic>