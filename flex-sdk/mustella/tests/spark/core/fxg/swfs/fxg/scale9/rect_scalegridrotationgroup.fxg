<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">
    <Group rotation="25">
        <Group x="50" y="10" scaleX="2.5" scaleY="2.5"
            scaleGridLeft="8" scaleGridTop="8" scaleGridRight="92" scaleGridBottom="17">
            <Rect width="100" height="25" radiusX="8" radiusY="8">
                <stroke>
                    <SolidColorStroke color="#000000" weight="1"/>
                </stroke>
                <fill>
                    <SolidColor color="#FF9900" />
                </fill>
            </Rect>
        </Group>
    </Group>
    <Group rotation="25">
        <Group x="50" y="100" scaleX="2.5" scaleY="2.5">
            <Rect width="100" height="25" radiusX="8" radiusY="8">
                <stroke>
                    <SolidColorStroke color="#000000" weight="1"/>
                </stroke>
                <fill>
                    <SolidColor color="#FF9900" />
                </fill>
            </Rect>
        </Group>
    </Group>
</Graphic>
