<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">
      <TextGraphic fontFamily="MyVeraSpark" fontSize="12" lineHeight="120%" kerning="on" x="50" y="20">
        <content><p><span>Test closepath (Z).</span></p>
        <p><span>M 50 80 L 150 180 M 250 80 Z</span></p></content>
      </TextGraphic>
        <Path winding="nonZero"  data="M 50 80 L 150 180 M 250 80 Z" >
          <stroke>
            <SolidColorStroke caps="none" weight="3" joints="miter" miterLimit="4"/>
          </stroke>
        </Path>
      <TextGraphic fontFamily="MyVeraSpark" fontSize="12" lineHeight="120%" kerning="on" x="50" y="220">
        <content><p><span>Test closepath (z).</span></p>
        <p><span>M 50 250 l 100 100 m 100 -100 z</span></p></content>
      </TextGraphic>
        <Path winding="nonZero"  data="M 50 250 l 100 100 m 100 -100 z" >
          <stroke>
            <SolidColorStroke caps="none" weight="3" joints="miter" miterLimit="4"/>
          </stroke>
        </Path>
     
</Graphic>