<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">

      <TextGraphic fontFamily="MyVeraSpark" fontSize="12" lineHeight="120%" kerning="on" x="50" y="100">
        <content><p><span>Test closepath (Z) in the middle of a path.</span></p>
        <p><span>M 50 150 L 150 250 L 250 150 Z L 350 250</span></p></content>
      </TextGraphic>
        <Path winding="nonZero"  data="M 50 150 L 150 250 L 250 150 Z L 350 250" >
          <stroke>
            <SolidColorStroke caps="none" weight="3" joints="miter" miterLimit="4"/>
          </stroke>
        </Path>        
      <TextGraphic fontFamily="MyVeraSpark" fontSize="12" lineHeight="120%" kerning="on" x="50" y="300">
        <content><p><span>Test closepath (z) in the middle of a path.</span></p>
        <p><span>m 50 350 l 100 100 l 100 -100 z l 100 100</span></p></content>
      </TextGraphic>
        <Path winding="nonZero"  data="m 50 350 l 100 100 l 100 -100 z l 100 100" >
          <stroke>
            <SolidColorStroke caps="none" weight="3" joints="miter" miterLimit="4"/>
          </stroke>
        </Path>        
</Graphic>