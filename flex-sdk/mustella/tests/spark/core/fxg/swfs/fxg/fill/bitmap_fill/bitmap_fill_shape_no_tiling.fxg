<!-- Warning: FXG Viewer doesn't render embedded file correctly. Illustrator does not honor the repeat attribute set to false                                                                                		-->
<!-- FXG 1.0 Test BitmapFill fills a shape with non-repeating bitmap image                   -->
<!-- The BitmapFill element fills a path or shape with an bitmap image.

By default, the bitmap image is used to paint a fill rectangle stretching from (0,0) at the top left 
to the (width,height) of the bitmap image, in the coordinate space of the shape's parent grouping element's 
coordinate system, transformed by any matrix transformation defined on the Fill.
   		
-->
<!-- Author : Vivek Agrawal  26 Feb-2009                       			-->
<!--                                                                       				-->
<!-- History:                                                              				-->
<!--  26-Feb-2009, created.                                         			-->
<!-- ==========================================	-->
<!-- ==========================================	-->
<Graphic version="1.0" xmlns="http://ns.adobe.com/fxg/2008">
	<Rect x="10" y="10" width="300" height="200">
		<fill>
			<BitmapFill x="150" y="10" scaleX="1.5" scaleY="1.5" rotation="225" repeat="false" source="@Embed('../../../../../../../../Assets/Images/next.JPG')"/>
		</fill>
		<stroke>
	  		<SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
		</stroke>
	</Rect>
	<Ellipse x="10" y="220" width="300" height="200">
		<fill>
			<BitmapFill x="150" y="10" scaleX="1.5" scaleY="1.5" rotation="225" repeat="false" source="@Embed('../../../../../../../../Assets/Images/next.JPG')"/>
		</fill>
		<stroke>
	  		<SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
		</stroke>
	</Ellipse>
</Graphic>