<!--                                                                                  		       -->
<!-- FXG 1.0 Tests ability to handle the 'rect' basic-shapes element with a color transformation . 
The first group shows a set of  4 rectangles inside a bordering rectangle with no transform applied. The second
group is the same 4 rectangles with color transforms applied to each of them with different parameters . 
This enables a user to study the shift from the first group  due to color transformation                                               .    -->
<!-- Author : Vivek Agrawal  3-March-2009                       			-->
<!--                                                                       				-->
<!-- History:                                                              				-->
<!--  3-March-2009, created.                                         			-->
<!-- ==========================================	-->
<!-- ==========================================	-->
<Graphic version="1.0" viewHeight="359" viewWidth="479" xmlns="http://ns.adobe.com/fxg/2008">
 <Group x="18" y="14">
      <Rect x="4" y="0.5" width="50" height="80">
        <stroke>
          <SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
        </stroke>       
      </Rect>
      <Rect x="104" y="0.5" width="50" height="80" >
        <fill>
          <SolidColor color="#ff0000"/>
        </fill>        
      </Rect>
       <Rect x="4" y="150.5" width="50" height="80">
        <stroke>
          <SolidColorStroke color="#0000ff" caps="none" weight="8" joints="miter" miterLimit="4"/>
        </stroke>
      </Rect>
      <Rect x="104" y="150.5" width="50" height="80">
        <fill>
          <SolidColor color="#00ff00"/>
        </fill>
        <stroke>
          <SolidColorStroke color="#0000ff" caps="none" weight="8" joints="miter" miterLimit="4"/>
        </stroke>		        
      </Rect>
    <Rect x="0.5" y="0.5" width="478" height="358">
      <stroke>
        <SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
      </stroke>  
    </Rect>
</Group>
 <Group x="18" y="14">
      <Rect x="4" y="0.5" width="50" height="80">
        <stroke>
          <SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
        </stroke>
		<transform>
			<Transform>
				<colorTransform>
					<ColorTransform alphaMultiplier="0.5" alphaOffset="0.3" blueMultiplier="0.2" blueOffset="10" greenMultiplier="0.5" greenOffset="30" redMultiplier="5" redOffset="20"/>
				</colorTransform>
			</Transform>
        </transform>		        
      </Rect>
      <Rect x="104" y="0.5" width="50" height="80" >
        <fill>
          <SolidColor color="#ff0000"/>
        </fill>
		<transform>
			<Transform>
				<colorTransform>
					<ColorTransform alphaMultiplier="0.5" alphaOffset="0.3" blueMultiplier="0.2" blueOffset="10" greenMultiplier="0.5" greenOffset="30" redMultiplier="5" redOffset="20"/>
				</colorTransform>
			</Transform>
        </transform>		        
      </Rect>
       <Rect x="4" y="150.5" width="50" height="80">
        <stroke>
          <SolidColorStroke color="#0000ff" caps="none" weight="8" joints="miter" miterLimit="4"/>
        </stroke>
		<transform>
			<Transform>
				<colorTransform>
					<ColorTransform alphaMultiplier="0.5" alphaOffset="0.3" blueMultiplier="0.2" blueOffset="10" greenMultiplier="0.5" greenOffset="30" redMultiplier="5" redOffset="20"/>
				</colorTransform>
			</Transform>
        </transform>		        
      </Rect>
      <Rect x="104" y="150.5" width="50" height="80">
        <fill>
          <SolidColor color="#00ff00"/>
        </fill>
        <stroke>
          <SolidColorStroke color="#0000ff" caps="none" weight="8" joints="miter" miterLimit="4"/>
        </stroke>
		<transform>
			<Transform>
				<colorTransform>
					<ColorTransform alphaMultiplier="0.5" alphaOffset="0.3" blueMultiplier="0.2" blueOffset="10" greenMultiplier="0.5" greenOffset="30" redMultiplier="5" redOffset="20"/>
				</colorTransform>
			</Transform>
        </transform>		        
      </Rect>
    <Rect x="0.5" y="0.5" width="478" height="358">
      <stroke>
        <SolidColorStroke caps="none" weight="1" joints="miter" miterLimit="4"/>
      </stroke>
		<transform>
			<Transform>
				<colorTransform>
					<ColorTransform alphaMultiplier="0.5" alphaOffset="0.3" blueMultiplier="0.2" blueOffset="10" greenMultiplier="0.5" greenOffset="30" redMultiplier="5" redOffset="20"/>
				</colorTransform>
			</Transform>
        </transform>		      
    </Rect>
</Group>
</Graphic>