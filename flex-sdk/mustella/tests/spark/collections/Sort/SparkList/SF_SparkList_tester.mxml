        <TestCase testID="SortField_sparkList_default_test_1" 
				  description="sort items with default property setting" 
				  keywords="[Sort, sparkList]">
            <setup>
                <ResetComponent target="uiListComp" className="newComps.CustomListComp" waitEvent="updateComplete" waitTarget="uiListComp"/>
                <SetStyle target="uiListComp.sparkCollator" styleName="locale" valueExpression="value='ru-RU'"/>
                <SetStyle target="uiListComp.mySort" styleName="locale" valueExpression="value='ja-JP'"/>
                <SetStyle target="uiListComp.mySortField" styleName="locale" valueExpression="value='ru-RU'"/>
                <SetStyle target="uiListComp.mySortField2" styleName="locale" valueExpression="value='ru-RU'"/>
                <SetProperty target="uiListComp" propertyName="listValueField" value="name"/>
                <SetProperty target="uiListComp" propertyName="listDataProvider" valueExpression="value=application.uiListComp.dataComp.dataArrCollS"/>
                <SetProperty target="uiListComp.mySortField" propertyName="name" value="name"/>
                <SetProperty target="uiListComp.mySortField2" propertyName="name" value="address"/>
                <SetProperty target="uiListComp.mySort" propertyName="fields" valueExpression="value=[application.uiListComp.mySortField, application.uiListComp.mySortField2]"/>
                <SetProperty target="uiListComp.listDataProvider" propertyName="sort" valueExpression="value=application.uiListComp.mySort"/>
                <RunCode code="application.uiListComp.listDataProvider.refresh()" waitEvent="collectionChange" waitTarget="uiListComp.listDataProvider"/>
            </setup>
            <body>
                <AssertStyleValue target="uiListComp.mySort" styleName="locale" valueExpression="value='ja-JP'"/>
                <AssertStyleValue target="uiListComp.mySortField" styleName="locale" valueExpression="value='ru-RU'"/>
                <AssertStyleValue target="uiListComp.mySortField2" styleName="locale" valueExpression="value='ru-RU'"/>
                <AssertMethodValue method="value=SFConstant.getArrayOfField('name',application.uiListComp.listDataProvider)" 
								   valueExpression="value=application.uiListComp.sparkSortedArray"/>
                <!--<AssertMethodValue method="value=SFConstant.getArrayOfField('address',application.uiListComp.listDataProvider)" 
								   valueExpression="value=application.uiListComp.sparkSortedArrayField('address')"/>-->
            </body>
        </TestCase>
