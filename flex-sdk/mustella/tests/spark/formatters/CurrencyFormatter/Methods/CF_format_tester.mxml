<!-- This test file is to test format() int, unit and number value when setting with different properties -->

<testCases>
	<!--test format method for inline and script cf object with no locale set -->
	<TestCase testID="CF_format_setNoLocaleInline_test1" description="test format method for inline and script cf object with no locale set" keywords="[CurrencyFormatter, format]">
		<setup>
			<RunCode code="cfGSLib=new flash.globalization.CurrencyFormatter('en')" />
			<Pause timeout="{CFConstant.pauseTime}"/>
		</setup>
		<body>
			<AssertMethodValue method="value=application.cfNoLocale.format(123456789.9876)" valueExpression="value=cfGSLib.format(123456789.9876)" />
			<AssertPropertyValue target="cfNoLocale" propertyName="lastOperationStatus" valueExpression="value=cfGSLib.lastOperationStatus"/>
			<AssertMethodValue method="value=application.cfNoLocale.format(0.345678)" valueExpression="value=cfGSLib.format(0.345678)" />
			<AssertPropertyValue target="cfNoLocale" propertyName="lastOperationStatus" valueExpression="value=cfGSLib.lastOperationStatus"/>
			<AssertMethodValue method="value=application.cfNoLocale.format(-100.04550000)" valueExpression="value=cfGSLib.format(-100.04550000)" />
			<AssertPropertyValue target="cfNoLocale" propertyName="lastOperationStatus" valueExpression="value=cfGSLib.lastOperationStatus"/>
			
			<AssertMethodValue method="value=application.cfNoLocaleScript.format(123456789.9876)" valueExpression="value=cfGSLib.format(123456789.9876)" />
			<AssertPropertyValue target="cfNoLocaleScript" propertyName="lastOperationStatus" valueExpression="value=cfGSLib.lastOperationStatus"/>
			<AssertMethodValue method="value=application.cfNoLocaleScript.format(-100.04550000)" valueExpression="value=cfGSLib.format(-100.04550000)" />
			<AssertPropertyValue target="cfNoLocaleScript" propertyName="lastOperationStatus" valueExpression="value=cfGSLib.lastOperationStatus"/>
		</body>
	</TestCase>
	
