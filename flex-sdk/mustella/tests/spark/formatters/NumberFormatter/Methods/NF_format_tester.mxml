<!-- This test file is to test format() int, unit and number value when setting with different properties -->

<testCases>
	<!--test format int value with default property seting  -->
	<TestCase testID="NF_format_defaultProperty_test1" 
			  description="test format int value with default property seting" keywords="[NumberFormatter, format]">
		<setup>
			<RunCode code="nfGSLib=new flash.globalization.NumberFormatter('en_US')" />
			<Pause timeout="{NFConstants.pauseTime}"/>
		</setup>
		<body>
			<AssertMethodValue method="value=application.nf.format(-1234567)" valueExpression="value=nfGSLib.formatNumber(-1234567)" />
			<AssertPropertyValue target="nf" propertyName="lastOperationStatus" valueExpression="value=nfGSLib.lastOperationStatus"/>
			<AssertMethodValue method="value=application.nf.format(0)" valueExpression="value=nfGSLib.formatNumber(0)" />
			<AssertPropertyValue target="nf" propertyName="lastOperationStatus" valueExpression="value=nfGSLib.lastOperationStatus"/>
			<AssertMethodValue method="value=application.nf.format(1234567)" valueExpression="value=nfGSLib.formatNumber(1234567)" />
			<AssertPropertyValue target="nf" propertyName="lastOperationStatus" valueExpression="value=nfGSLib.lastOperationStatus"/>
			<AssertMethodValue method="value=application.nf.format(1.3e10)" valueExpression="value=nfGSLib.formatNumber(1.3e10)" />
			<AssertPropertyValue target="nf" propertyName="lastOperationStatus" valueExpression="value=nfGSLib.lastOperationStatus"/>
			<AssertMethodValue method="value=application.nf.format(1.6e-100)" valueExpression="value=nfGSLib.formatNumber(1.6e-100)" />
			<AssertPropertyValue target="nf" propertyName="lastOperationStatus" valueExpression="value=nfGSLib.lastOperationStatus"/>
		</body>
	</TestCase>
	
