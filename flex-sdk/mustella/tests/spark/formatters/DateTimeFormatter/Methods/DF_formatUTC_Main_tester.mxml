	<!--test case for format an object : null or any Date object  -->
	<TestCase testID="DF_format_formatObject_test99" description="test case for format an object : null or any Date object " keywords="[DateTimeFormatter, format]">
		<setup>
			<RunCode code="flashDF=new flash.globalization.DateTimeFormatter('en-US','long','long')" />
			<Pause timeout="200"/>
		</setup>
		<body>
			<AssertMethodValue method="value=application.dfTestObj.format(null)" valueExpression="value=DFConstants.ERROT_TEXT"/>
			<AssertPropertyValue target="dfTestObj" propertyName="lastOperationStatus" valueExpression="value=DFConstants.noErrorMsg"/>
			<AssertMethodValue method="value=application.dfTestObj.format(new Date('notaDate'))" valueExpression="value=flashDF.format(new Date('notaDate'))"/>
			<AssertPropertyValue target="dfTestObj" propertyName="lastOperationStatus" valueExpression="value=DFConstants.illegalParamMsg"/>
			<AssertMethodValue method="value=application.dfTestObj.format( new Object() )" valueExpression="value=flashDF.format( new Date( new Object() ))"/>
			<AssertPropertyValue target="dfTestObj" propertyName="lastOperationStatus" valueExpression="value=flashDF.lastOperationStatus"/>
			<AssertMethodValue method="value=application.dfTestObj.format(application.dateValue)" valueExpression="value=flashDF.format(new Date(2000, 0, 1))"/>
			<AssertPropertyValue target="dfTestObj" propertyName="lastOperationStatus" value="noError"/>
		</body>
	</TestCase>
	
