<UnitTester testDir="spark/formatters/DateTimeFormatter/Methods/" 
			xmlns:mx="http://www.adobe.com/2006/mxml" 
			xmlns:s="library://ns.adobe.com/flex/spark" 
			xmlns="*" 
			testSWF="DF_formatUTC_Main.mxml">
  
	<mx:Script>
    <![CDATA[
		import flash.globalization.*;
		import assets.*;

		public var flashDF:flash.globalization.DateTimeFormatter;
		[Bindable]
		public var aDate:Date = new Date(2000, 0, 1);
  
		public static function init(o:DisplayObject):void
        {
			
        }
    ]]>
    </mx:Script>
  
	<mx:Metadata>
    <![CDATA[
        [Mixin]
    ]]>
  	</mx:Metadata>
	
<!--This test file is to test formatUTC() when a DateTimeFormatter is set with different dateStyle,timeStyle and Locale -->
<testCases>
	<!--testcase1-16 are used to test formatUTC  with "en_US" locale-->
		<TestCase testID="DF_formatUTC_en_US_long_long_test1" description="'en_US', 'long', 'long'" keywords="[DateTimeFormatter, formatUTC]">
		<setup>	
			<RunCode code="flashDF=new flash.globalization.DateTimeFormatter('en_US', 'long', 'long')" />
			<Pause timeout="200"/>
		</setup>
		<body>
			<SetStyle styleName="locale" target="FormatterLL" value="en_US" waitEvent="change"/>
			<SetProperty propertyName="dateStyle" target="FormatterLL" value="long" />
			<SetProperty propertyName="timeStyle" target="FormatterLL" value="long" />
			<Pause timeout="200"/>
			<SetProperty propertyName="useUTC" target="FormatterLL" valueExpression="value=true" waitEvent="change"/>
			<AssertMethodValue method="value=application.FormatterLL.format(aDate)" valueExpression="value=flashDF.formatUTC(aDate)"/>		
			<AssertPropertyValue propertyName="lastOperationStatus" target="FormatterLL" valueExpression="value=flashDF.lastOperationStatus"/>			
		</body>		
	</TestCase>
	
