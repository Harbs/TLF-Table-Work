<testCases>
	<!--the test case is for test default locale, the actual locale name is "en-US" -->
	<!--Set default locale, i-default -->
	<TestCase testID="CLT_actualLocaleIDName_i_default_inline_test1" description="Set default locale, i-default" keywords="[Collator, actualLocaleIDName]">
		<setup>
			<RunCode code="flashCL=new flash.globalization.Collator('i-default')" />
			<Pause timeout="{CollatorTestConstants.pauseTime}"/>
		</setup>
		<body>
			<SetStyle styleName="locale" target="clt_inline" value="i-default" waitEvent="change"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=flashCL.lastOperationStatus" target="clt_inline"/>
			<AssertPropertyValue propertyName="actualLocaleIDName" valueExpression="value=flashCL.actualLocaleIDName" target="clt_inline"/>
		</body>
	</TestCase>
	
