	<!--test SortingCollator instance with all basic properties set to default values-->
	<TestCase testID="SortingCollator_compare_defalult_script_test1" description="test SortingCollator instance with all basic properties set to default values" keywords="[SortingCollator, compare]">
		<setup>
			<RunCode code="cltGSLib = new flash.globalization.Collator('zh-CN','sorting')"/>
			<Pause timeout="{CollatorTestConstants.pauseTime}" />
			<SetStyle target="clt_sorting" styleName="locale" value="zh-CN" waitEvent="change"/>
		</setup>
		<body>
			<!--test ignoreCase property with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('Ä','ä')" valueExpression="value=cltGSLib.compare('Ä','ä')"/>
			<AssertMethodValue method="value=application.clt_sorting.compare('ae','Æ')" valueExpression="value=cltGSLib.compare('ae','Æ')"/>
			<AssertMethodValue method="value=application.clt_sorting.compare('SS','ß')" valueExpression="value=cltGSLib.compare('SS','ß')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test ignoreCharacterwidth property with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('ァ','ｧ')" valueExpression="value=cltGSLib.compare('ァ','ｧ')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test ignoreDiacritics property with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('côte','coté')" valueExpression="value=cltGSLib.compare('côte','coté')"/>
			<AssertMethodValue method="value=application.clt_sorting.compare('ぁ','あ')" valueExpression="value=cltGSLib.compare('ぁ','あ')"/>
			<AssertMethodValue method="value=application.clt_sorting.compare('Ô','Ö')" valueExpression="value=cltGSLib.compare('Ô','Ö')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test ignoreKanatype property with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('ぁ','ァ')" valueExpression="value=cltGSLib.compare('ぁ','ァ')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test ignoreSymbols property with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('AbC 2','aBC ! @# 2')" valueExpression="value=cltGSLib.compare('AbC 2','aBC ! @# 2')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test numericComparison property with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('version2','version10')" valueExpression="value=cltGSLib.compare('version2','version10')"/>
			<AssertMethodValue method="value=application.clt_sorting.compare('z2.txt','z15.txt')" valueExpression="value=cltGSLib.compare('z2.txt','z15.txt')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test always different string with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('o','0')" valueExpression="value=cltGSLib.compare('o','0')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test always same string with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('ae','æ')" valueExpression="value=cltGSLib.compare('ae','æ')"/>
			<AssertMethodValue method="value=application.clt_sorting.compare('ss','ß')" valueExpression="value=cltGSLib.compare('ss','ß')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test ignoreDiacritcs, CharacterWidth and Kanatype properties with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('あ','ｧ')" valueExpression="value=cltGSLib.compare('あ','ｧ')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
			
			<!--test ignoreCase, Diacritcs and CharacterWidth properties with default value-->
			<AssertMethodValue method="value=application.clt_sorting.compare('Ä','ａ')" valueExpression="value=cltGSLib.compare('Ä','ａ')"/>
			<AssertPropertyValue propertyName="lastOperationStatus" valueExpression="value=cltGSLib.lastOperationStatus" target="clt_sorting"/>
		</body>
	</TestCase>	
	
