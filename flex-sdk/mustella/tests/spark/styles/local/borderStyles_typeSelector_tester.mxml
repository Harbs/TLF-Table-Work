<UnitTester testDir="spark/styles/local/" 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
    xmlns:mx="library://ns.adobe.com/flex/mx" 
    xmlns:s="library://ns.adobe.com/flex/spark"
    xmlns="*"
    testSWF="borderStyles_typeSelector_main.mxml">
    
    <!-- Default code for all tests -->
    <fx:Script>
        <![CDATA[         
        	public static function init(o:DisplayObject):void {}
        ]]>
    </fx:Script>

    <fx:Metadata>
    <![CDATA[
        [Mixin]
    ]]>
    </fx:Metadata>
           
    <fx:Style>
    	.myStyle{
			borderColor: #00FF00;
			borderAlpha: 0.7;
		}
		
		.noBorder{
			borderVisible: false;
		}
		
		.showBorder{
			borderVisible: true;
		}
    </fx:Style>
    <testCases>    

  		<TestCase testID="borderColor_classSelector" keywords="[borderColor]" description="Test borderColor and borderAlpha applies to all components">
  			<setup>
				<ResetComponent target="bi" className="comps.borderItems" waitEvent="updateComplete"/>
			</setup>
			<body>
  				<CompareBitmap target="bi" url="../local/baselines/$testID.png" />
  			</body>
  		</TestCase>
 		
