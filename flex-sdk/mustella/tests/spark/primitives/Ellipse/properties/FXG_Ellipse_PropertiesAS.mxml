        <TestCase testID="FXG_Ellipse_height_positive" description="Ellipse height is set to a positive int" keywords="[Ellipse, height, Graphic]">
            <setup>
                <ResetComponent target="graphic1" className="comps.EllipseBasic" waitTarget="graphic1" waitEvent="updateComplete" />
            </setup>
            <body>
