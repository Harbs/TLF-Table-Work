    <testCases>
    <!-- LineSegment x,y -->
        <TestCase testID="GraphicTags_PathLineSegment_x_positive" description="LineSegment x set to a positive int" keywords="[Path, LineSegment, Graphic]">
            <setup>
                <ResetComponent target="graphic1" className="comps.PathBasic" waitTarget="graphic1" waitEvent="updateComplete" />
            </setup>
            <body>
                <SetProperty target="graphic1.path1" propertyName="data" value="M 40 140 L 120 140 C 70 0 10 80 75 90 Q -10 150 190 115" waitTarget="graphic1.getChildAt(0)" waitEvent="updateComplete" />
                <CompareBitmap url="../Properties/baselines/$testID.png" target="graphic1.getChildAt(0)" />
