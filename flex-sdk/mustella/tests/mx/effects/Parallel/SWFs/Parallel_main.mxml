        </mx:Parallel>
        <mx:Parallel id="p_compositeDuration1">
            <s:Move duration="1000"/>
        </mx:Parallel>
        <mx:Parallel id="p_compositeDuration2">
            <s:Move startDelay="500" duration="1000"/>
        </mx:Parallel>
        <mx:Parallel id="p_compositeDuration3">
