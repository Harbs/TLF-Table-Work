    <testCases>

        <!-- Blur -->
        <TestCase testID="Sequence_seek_Blur" keywords="[Sequence, Seek, Blur]" description="Seek the Blur effect.">
            <setup>
                <RunCode code="application.Reset()" waitTarget="dispatcher" waitEvent="refreshComplete" timeout="5000"/>
            </setup>
            <body>
                <RunCode code="application.ClickButton('Sequence_seek_Blur')" waitTarget="timer" waitEvent="timerComplete" />
                <CompareBitmap target="currentCanvas" url="../seek/Baselines/Sequence_seek_Blur.png" />
            </body>
        </TestCase>
