        <![CDATA[
        import mx.core.Application;
        
        public static function init(o:DisplayObject):void{}

        ]]>
    </mx:Script>
    <mx:Metadata><![CDATA[[Mixin]]]></mx:Metadata>
    <testCases>

        <TestCase testID="Blur_Button" keywords="[Blur, object, Button]" description="Test Blur on a Button.">
            <setup>
                <RunCode code="application.Reset()" />
                <Pause timeout="1500"/>
            </setup>
            <body>
                <RunCode code="application.SeekControl('Button', application.eff_Blur_X, 1000, 0, 1, 0, 500)" waitTarget="timer" waitEvent="timerComplete" />
                <CompareBitmap target="currentCanvas" url="../object/Baselines/$testID.png" />
            </body>
        </TestCase>
