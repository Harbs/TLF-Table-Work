    <testCases>

        <!-- Blur -->
        <TestCase testID="Effect_seek_Blur" keywords="[Effect, Seek, Blur]" description="Seek the Blur effect.">
            <setup>
                <RunCode code="application.Reset()" waitTarget="dispatcher" waitEvent="refreshComplete" />
            </setup>
            <body>
                <RunCode code="application.ClickButton('Effect_seek_Blur')" waitTarget="timer" waitEvent="timerComplete" timeout="5000" />
                <CompareBitmap target="currentCanvas" url="../seek/Baselines/$testID.png" />
            </body>
        </TestCase>
