<TestCase testID="addItem_vlist_test5" keywords="[AsyncListView,addItem]" description="Add when new item won't be in view, all items pending." >
	<setup>
		<ResetComponent target="obj1" className="assets.PagedDataComp1" waitEvent="updateComplete" />
	</setup>
	<body>
		<RunCode code="FlexGlobals.topLevelApplication.obj1.setup(0,30)" waitTarget="obj1.theList.dataGroup" waitEvent="updateComplete" />
		<RunCode code="savedObject = {info: 'added item', status: 'present'}" />
		<RunCode code="FlexGlobals.topLevelApplication.obj1.alv.addItem(savedObject)" />	
		<AssertMethodValue method="value = FlexGlobals.topLevelApplication.verifyALVItems(0, 29, 'pending', FlexGlobals.topLevelApplication.obj1.alv)" value="pass" />
		<AssertMethodValue method="value = FlexGlobals.topLevelApplication.obj1.alv.getItemAt(30).info" value="added item" />
		<AssertMethodValue method="value = FlexGlobals.topLevelApplication.obj1.alv.getItemIndex(FlexGlobals.topLevelApplication.obj1.alv.getItemAt(0))" value="0" />
