        <fx:Script>
            <![CDATA[
		import mx.core.ILayoutElement;
		                
		private var m:Matrix;
		                
		private function transformButton(a:Number, b:Number, c:Number, d:Number, tx:Number, ty:Number):Matrix{
			var m1:Matrix = new Matrix();
			m1.a = a;
			m1.b = b;
			m1.c = c;
			m1.d = d;
			m1.tx = tx;
			m1.ty = ty;
			return m1;
                }
                
                private var compValue:Boolean;
                
                private function compFunc(actual:Number, expected:Number):Boolean
                {
                	compValue=false;
                	if (actual == expected)
                		compValue=true;
                	else
                		trace("Actual " + actual);
                	
               	return compValue;

	       	}
            ]]>
        </fx:Script>

