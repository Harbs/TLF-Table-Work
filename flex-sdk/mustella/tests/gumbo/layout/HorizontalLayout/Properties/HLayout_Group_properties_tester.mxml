		<TestCase testID="HLayout_group_clipAndEnableScrolling_default" keywords="[HorizontalLayout,clipAndEnableScrolling]" description="Defaults to false." >
			<setup>
				<ResetComponent target="myScroll" className="Assets.MyGroupScroller" waitEvent="updateComplete" />
				<ResetComponent target="grp" className="Assets.MyGroup" waitEvent="updateComplete" />
			</setup>
			<body>
				<AssertPropertyValue propertyName="clipAndEnableScrolling" target="grp" value="false" />
			</body>
		</TestCase>

		<TestCase testID="HLayout_group_clipAndEnableScrolling_false" keywords="[HorizontalLayout,clipAndEnableScrolling]" description="No clipping occurs" >
		    <setup>
				<ResetComponent target="grp" className="Assets.MyGroup" waitEvent="updateComplete" />
				<ResetComponent target="myScroll" className="Assets.MyGroupScroller" waitEvent="updateComplete" />
				<SetProperty propertyName="visible" value="false" target="myScroll"/>
		    </setup>
		    <body>
				<SetProperty propertyName="height" value="200" target="grp"/>
				<SetProperty propertyName="width" value="200" target="grp" waitEvent="updateComplete" waitTarget="grp"/>
	
				<RunCode code="application.addRect(application.grp)"/>
				<SetProperty propertyName="width" target="grp.getElementAt(0)" value="200" waitEvent="updateComplete" waitTarget="grp"/>
				<SetProperty propertyName="height" target="grp.getElementAt(0)" value="150" waitEvent="updateComplete" waitTarget="grp"/>
				
				<AssertPropertyValue propertyName="width" value="200" target="grp"/>
