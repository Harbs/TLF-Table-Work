				<AssertPropertyValue propertyName="contentWidth" value="200" target="grp"/>
				<AssertPropertyValue propertyName="contentHeight" value="150" target="grp"/>
				
				<CompareBitmap url="../Properties/baselines/$testID.png" numColorVariances="8" maxColorVariance="5"  target=""/>
		    </body>
		</TestCase>

