<s:Application backgroundColor="0xFFFFFF" height="375" width="500" 

	xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:mx="library://ns.adobe.com/flex/mx" xmlns:s="library://ns.adobe.com/flex/spark"  xmlns:local="comps.*">

	<fx:Style>
        @namespace s "library://ns.adobe.com/flex/spark";
        @namespace mx "library://ns.adobe.com/flex/mx";

        @font-face{
            src: url("../../../../../Assets/Fonts/Open_Sans/OpenSans-Regular.ttf");
            fontFamily: myArial;
            embedAsCFF: true;
        }


        @font-face{
            src: url("../../../../../Assets/Fonts/Open_Sans/OpenSans-Bold.ttf");
            fontWeight: bold;
            fontFamily: myArial;
            embedAsCFF: true;
        }

        @font-face{
            src: url("../../../../../Assets/Fonts/Open_Sans/OpenSans-Italic.ttf");
            fontStyle: italic;
            fontFamily: myArial;
            embedAsCFF: true;

        }

        global{
            fontAntiAliasType: "normal";
            fontFamily: myArial;
            fontSize: 12;
            fontLookup: "embeddedCFF";
        }
		
	</fx:Style>

	<s:layout>

		<s:VerticalLayout/>

	</s:layout>

	

	<local:MyGroup id="pan"/>

	

</s:Application>

