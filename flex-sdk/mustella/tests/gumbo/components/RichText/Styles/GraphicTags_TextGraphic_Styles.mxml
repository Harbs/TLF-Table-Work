            import spark.components.RichText;
	    import flashx.textLayout.elements.TextFlow;

            import flash.text.engine.*;

            import flashx.textLayout.formats.*;
            import spark.utils.TextFlowUtil;

            public var singleLineText:String = "The quick brown fox jumps over the lazy dog.";
            public var multiLineText:String = "The quick brown\nfox jumps over\nthe lazy dog.";
            public var multiLineTextLong:String = "The quick brown fox jumps over the lazy dog.\nThe quick brown fox jumps over the lazy dog.";
            public var lorem:String = "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec nec mauris. Curabitur nisl lorem, auctor in, condimentum sed, aliquam id, felis. \nSed bibendum, nisi nec pellentesque volutpat, neque metus consequat mi, quis molestie dolor lorem at nunc. Vestibulum quis dolor. Suspendisse eget sapien. Etiam in risus vel diam malesuada tincidunt. Sed orci dui, pellentesque vel, cursus vel, aliquam eu, erat. Donec eget purus eu diam porta lobortis. In nec neque. Vivamus nisi.";
            public var lorem1:String = "The quick brown fox jumps over the lazy dog. 1234567890,Lorem ipsum dolor sit amet";
            public var singleLineNumbers:String = "The quick brown fox jumps over the lazy dog. 1234567890";
            public var ligatureLevelText:String = "fi fj fl ft ffi ffj ffl ct st Th";
            public var kerningText:String = "YMMVAWAWAWAWA";


            public var content_baselineShift_subscript:String = "The quick brown <span baselineShift='subscript'>subscript</span> jumps over the lazy dog.";
            public var content_baselineShift_subscript2:String = "<p baselineShift='subscript'>The quick brown <span baselineShift='0'>subscript</span> jumps over the lazy dog.</p>";

            public var content_baselineShift_superscript:String = "The quick brown <span baselineShift='superscript'>superscript</span> jumps over the lazy dog.";
            public var content_baselineShift_superscript2:String = "<p baselineShift='superscript'>The quick brown <span baselineShift='0'>superscript</span> jumps over the lazy dog.</p>";

            public var content_fontWeight_bold:String = "The quick brown <span fontWeight='bold'>fox</span> jumps over the lazy dog.";
            public var content_fontWeight_bold2:String = "<p fontWeight='bold'>The quick brown <span fontWeight='normal'>fox</span> jumps over the lazy dog.</p>";

            public var content_fontStyle_italic:String = "The quick brown <span fontStyle='italic'>fox</span> jumps over the lazy dog.";
            public var content_fontStyle_italic2:String = "<p fontStyle='italic'>The quick brown <span fontStyle='normal'>fox</span> jumps over the lazy dog.</p>";
	    public var content_backgroundColor :String = "<p fontStyle='italic'>The quick brown <span fontStyle='normal' backgroundColor='0xFF0000' backgroundAlpha='0.5' >fox</span> jumps over the lazy dog.</p>";

        ]]>
    </fx:Script>

    <testCases>

        <!-- style: alignmentBaseline -->
        <TestCase testID="GraphicTags_TextGraphic_alignmentBaseline_default" description="Check default value of alignmentBaseline style" keywords="[TextGraphic, alignmentBaseline, Graphic]">
            <setup>
                <ResetComponent target="textGraphicComp" className="comps.TextGraphicBasic2" waitTarget="textGraphicComp" waitEvent="updateComplete" />
                <SetProperty target="textGraphicComp.textGraphic" propertyName="text" valueExpression="value=multiLineText;" waitTarget="textGraphicComp.textGraphic" waitEvent="updateComplete" />
            </setup>
            <body>
                <AssertStyleValue target="textGraphicComp.textGraphic" styleName="alignmentBaseline" value="useDominantBaseline" />
                <CompareBitmap numColorVariances="5" maxColorVariance="15"  url="../Styles/baselines/$testID.png" target="container" />
            </body>
        </TestCase>

