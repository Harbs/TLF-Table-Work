	<fx:Script>
		<![CDATA[
			import comps.*;
			
			import flash.geom.Point;
			import flash.text.engine.*;
			
			import flashx.textLayout.container.TextContainerManager;
			import flashx.textLayout.conversion.TextConverter;
			import flashx.textLayout.elements.IConfiguration;
			import flashx.textLayout.elements.TextFlow;
			import flashx.textLayout.formats.*;
			import flashx.textLayout.formats.LineBreak;
			
			import mx.core.FlexGlobals;
			
			import spark.components.RichEditableText;
			import spark.utils.TextFlowUtil;
			public var startPos:Point, toPos:Point;
			
			protected function doCoverageMethods(target:RichEditableText):void
			{
				
				
				trace(RichEditableText(target).isTruncated);
				trace(RichEditableText(target).lineBreak);
				trace(RichEditableText(target).heightInLines);
				var content:Object = RichEditableText(target).content;
				trace(content);
				var restrictIs:String = RichEditableText(target).restrict;
				trace(restrictIs);
				var imemode:String = RichEditableText(target).imeMode;
				trace(imemode);
				var mxChars:int = RichEditableText(target).maxChars;
				trace(mxChars);
				var selectableBool:Boolean = RichEditableText(target).selectable;
				trace(selectableBool);
				var typText:String = RichEditableText(target).typicalText;
				trace(typText);
				var tcm:TextContainerManager = new TextContainerManager(RichEditableText(target));
				//trace(mx.utils.ObjectUtil.toString(tcm));	
				RichEditableText(target).restrict = "";
				trace(RichEditableText(target).restrict);
				RichEditableText(target).imeMode = IME.conversionMode;
				RichEditableText(target).maxChars = 10;
				trace(RichEditableText(target).maxChars);
				RichEditableText(target).selectable = false;
				RichEditableText(target).selectable = true;
				trace(RichEditableText(target).selectable);
				RichEditableText(target).typicalText = "typicalText";
				RichEditableText(target).selectAll();
				var sap:int = RichEditableText(target).selectionActivePosition;
				trace("selectionActivePosition: " + sap);
			}
			
		]]>
	</fx:Script>

	<testCases>
		
		<TestCase testID="AdditionalCoverage" keywords="[TLF 2.0 Integration, RichEditableText]" 
				  description="test getter/setter methods for code coverage.">
			<setup>
				<ResetComponent target="customRETEditor" className="TLF.comp.CustomRETEditor" waitEvent="updateComplete" waitTarget="customRETEditor"/>
				<SetProperty target="customRETEditor.editor" propertyName="height" value="600" waitTarget="customRETEditor.editor" waitEvent="updateComplete" />
				<SetProperty target="customRETEditor.editor" propertyName="width" value="500" waitTarget="customRETEditor.editor" waitEvent="updateComplete" />
				<SetProperty target="customRETEditor.editor" propertyName="maxHeight" value="600" waitTarget="customRETEditor.editor" waitEvent="updateComplete" />
				<SetProperty target="customRETEditor.editor" propertyName="maxWidth" value="500" waitTarget="customRETEditor.editor" waitEvent="updateComplete" />
				
				<RunCode code="application.customRETEditor.editor.setFocus()" waitEvent="focusIn" waitTarget="customRETEditor.editor" />
				<SetProperty target="customRETEditor.editor" propertyName="textFlow" valueExpression="value=application.TextFlowGroup.shallowCopy() as TextFlow" waitTarget="customRETEditor.editor" waitEvent="updateComplete" />
				
				<RunCode code="doCoverageMethods(application.customRETEditor.editor)" waitEvent="updateComplete" waitTarget="customRETEditor.editor" />
			</setup>
			<body>
				<AssertPropertyValue target="customRETEditor.editor" propertyName="selectionActivePosition" value="31"/>
			</body>
		</TestCase>
		
