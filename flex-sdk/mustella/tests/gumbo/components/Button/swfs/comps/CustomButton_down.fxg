<Graphic xmlns:fc="http://ns.adobe.com/flashcatalyst/2009" xmlns:d="http://ns.adobe.com/fxg/2008/dt" xmlns="http://ns.adobe.com/fxg/2008" version="2.0"
	viewWidth="126" viewHeight="61"
	scaleGridLeft="5" scaleGridTop="5" scaleGridRight="121" scaleGridBottom="56">
	
  	<!-- main black border -->
  	<Rect x="0" y="0" height="61" radiusX="10" radiusY="10" width="126">
		<stroke>
			<SolidColorStroke weight="1" alpha="0.80" color="#030303"/>
		</stroke>
	</Rect>

	<!-- inner shadow begin -->
	<!-- outermost, inner shadow -->
  	<Rect x="1" y="1" height="60" radiusX="10" radiusY="10" width="124">
		<stroke>
			<LinearGradientStroke rotation="90" weight="1" >
				<GradientEntry alpha="0.40" color="#000000" ratio="0.0"/>
				<GradientEntry alpha="0.00" color="#000000" ratio="1.0"/>
			</LinearGradientStroke>
		</stroke>
	</Rect>

	<!-- middle shadow -->
  	<Rect x="2" y="2" height="57" radiusX="10" radiusY="10" width="122">
		<stroke>
			<LinearGradientStroke rotation="90" weight="1">
				<GradientEntry alpha="0.15" color="#000000" ratio="0.0"/>
				<GradientEntry alpha="0.00" color="#000000" ratio="1.0"/>
			</LinearGradientStroke>
		</stroke>
	</Rect>

	<!-- inner, transparent filled rect -->
  	<Rect x="1" y="1" height="60" radiusX="10" radiusY="10" width="124">
		<fill>
        	<LinearGradient rotation="90">
				<GradientEntry alpha="0.20" color="#000000" ratio="0.02"/>
				<GradientEntry alpha="0.40" color="#000000" ratio="1.0"/>
        	</LinearGradient>			
		</fill>
	</Rect>

	<!-- bottom outside halo line -->
  	<Rect x="1" y="62" height="0" width="124">
		<stroke>
			<SolidColorStroke color="#FFFFFF" weight="1" alpha=".12"/>
		</stroke>
	</Rect>

</Graphic>