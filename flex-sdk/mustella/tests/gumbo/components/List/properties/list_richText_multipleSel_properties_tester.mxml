        <TestCase testID="RT_List_ms_property_arrayCollection__selectedIndex_dup0" keywords="[List, property,selectedIndex]" description="">
            <setup>
                <ResetComponent target="list1" className="spark.components.List" waitEvent="updateComplete" />
            	<RunCode code="application.list1.itemRenderer=new ClassFactory(comps.RichTextItemRenderer);"  />

		</setup>
