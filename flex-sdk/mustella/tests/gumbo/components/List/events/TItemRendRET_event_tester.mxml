        <TestCase testID="TRendRET_List_keyboard_navigation1" keywords="[List, keyboard]" description="">
            <setup>
                <ResetComponent target="list6" className="comps.TListComp" waitEvent="updateComplete" />
	    </setup>
            <body>
                <DispatchKeyEvent keys="[TAB, TAB, TAB, TAB, TAB, TAB]" waitTarget="list6" waitEvent="focusIn" />
                <DispatchKeyEvent keys="[PAGE_DOWN]"  waitTarget="list6" waitEvent="updateComplete"/>
                <DispatchKeyEvent keys="[PAGE_DOWN]"  waitTarget="list6" waitEvent="updateComplete"/>
                <DispatchKeyEvent keys="[PAGE_DOWN]"  waitTarget="list6" waitEvent="updateComplete"/>
                <DispatchKeyEvent keys="[PAGE_DOWN]"  waitTarget="list6" waitEvent="updateComplete"/>
                <DispatchKeyEvent keys="[PAGE_UP]"  waitTarget="list6" waitEvent="updateComplete"/>

                <CompareBitmap numColorVariances="10" ignoreMaxColorVariance="true" url="../events/baselines/$testID.png" target="list6" />
            </body>
        </TestCase>
