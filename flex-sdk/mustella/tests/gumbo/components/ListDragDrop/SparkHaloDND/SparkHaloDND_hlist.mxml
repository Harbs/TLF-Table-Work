                <SetProperty target="l1" propertyName="allowMultipleSelection" value="true" />
		<SetProperty target="l1" propertyName="width" value="200" />
            </setup>
            <body>
		<DispatchMouseEvent target="l1"  type="mouseUp"  localX="10" localY="10" />
