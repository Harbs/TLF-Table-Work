    <testCases>
        <TestCase testID="LinkButton_Styles_SkinStates_Embeding_JPG" keywords="[enabled, SkinStates,EmbedJPG, LinkButton]" >
            <setup>
                <ResetComponent target="myLinkButton" className="mx.controls.LinkButton"  waitEvent="updateComplete" waitTarget="myLinkButton"/>   
                <SetProperty target="myLinkButton" propertyName="label" value="Test Button" waitEvent="updateComplete" waitTarget="myLinkButton" />
                <SetStyle target="myLinkButton" styleName="styleName" value="myLinkButtonJPG" waitEvent="updateComplete" waitTarget="myLinkButton" /> 
                <WaitForEffectsToEnd />
            </setup>
            <body>
                <CompareBitmap target="myLinkButton" url="../Styles/baselines/" timeout="5000" >
					<ConditionalValue device="air" />
				</CompareBitmap>
            </body>
        </TestCase> 
