    import mx.collections.ArrayCollection;
    private function hideIt(value:Boolean):void
    {
       application.dg.columns[1].visible=value
       application.dg.selectedIndex = application.dg.columns.length;
    }

    private function getItem():void
    {
     if(application.dg.dataProvider.getItemAt(0).Album=="Album A")
     {
        application.dg.selectedIndex=9;
     }
    }
    private function removeBottom():void
    {
           application.dg.dataProvider.removeItemAt(4);

    }
    private function removeTop():void
    {
       application.dg.dataProvider.removeItemAt(0);
    }
    public function setDPagain():void {
        var smallArray:Array
         smallArray=
         [
           { Artist:'Artist A', Album:'Album A', Price:11.99, Rating:'Excellent' },
           { Artist:'Artist A', Album:'Album B', Price:10.99, Rating:'Excellent' },
           { Artist:'Artist A', Album:'Album C', Price:12.99, Rating:'Excellent' },
           { Artist:'Artist A', Album:'Album D', Price:11.99, Rating:'Good' }


         ];

       application.dg.dataProvider = smallArray;
     }
     private function setit():void
    {
           application.dg.dataProvider.removeItemAt(2);

    }
    private function removeItemAt():void
    {
       application.dg.dataProvider.removeItemAt(2);

    }
    private function removeAll():void
    {
       application.dg.dataProvider.removeAll();

    }
    private function setWidth():void
    {
       application.dg.width=100

    }
    private function addItem():void
    {
       application.dg.dataProvider.addItem( { Artist:'Artist A', Album:'Album D', Price:11.99, Rating:'ugly' })
    }
    private function setHeaderRenderer():void
    {
       application.dg.columns[1].headerRenderer = new ClassFactory(mx.controls.Text)
    }
    private function setItemRenderer():void
    {
        application.dg.columns[1].itemRenderer = new ClassFactory(mx.controls.NumericStepper)
    }
        private function setColumnStyle():void
    {
        application.dg.columns[1].setStyle("color",0xCC0000)
    }
    private function createCollection():void
    {
      var ac:ArrayCollection = new ArrayCollection()
      for(var i:int=0;i<10;i++)
      {
        var obj:Object={a:"a"+i,b:"b"+i,c:"c"+i,d:"d"+i,e:"e"+i,f:"f"+i};
        ac.addItem(obj)
      }
      application.dg.dataProvider=ac;
    }

    ]]>
