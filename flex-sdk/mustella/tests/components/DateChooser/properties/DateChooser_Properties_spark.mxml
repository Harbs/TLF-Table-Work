	<mx:Script>
	<![CDATA[

	import mx.controls.DateChooser;

	public var rStart:Date = new Date();

	public var rEnd:Date = new Date(new Date().getFullYear(),new Date().getMonth(),new Date().getDate()+30);

	public function setDayNames():void
	{
		application.dcc.dc.dayNames = ["Sun","Mon","Tue","Wed","Thu","Fri","Sat"];
	}
	public function setDisabledDays():void
	{
		application.dcc.dc.disabledDays = [0,4];
	}
	public function setYearNavigationEnabled():void{
		application.dcc.dc.yearNavigationEnabled=true;
	}
	public function setShowToday():void{
		application.dcc.dc.showToday=false;
	}
	public function setFirstDayOfWeek():void{
		application.dcc.dc.firstDayOfWeek=4;
	}
	public function f():void{
		application.dcc.dc.displayedYear=2005;
	}
	public function setDisplayedMonth():void{
		application.dcc.dc.displayedMonth=5;
	}
	public function setDisplayedYear():void{
		application.dcc.dc.displayedYear=2005;
	}
	public function setMonthNames():void
	{
		application.dcc.dc.monthNames = ['Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'];
	}
	public function setSelectableRange():void{
		application.dcc.dc.selectableRange=[{rangeStart: new Date(), rangeEnd: rEnd}];
	}
	public function setDisabledRanges():void{
		application.dcc.dc.disabledRanges=[{rangeStart: rStart, rangeEnd: new Date(new Date().getFullYear(),new Date().getMonth(),(new Date().getDate()+30))}];
	}
	public function setScaleY():void{
		application.dcc.dc.scaleY=1.2;
	}
	public function setScaleX():void{
		application.dcc.dc.scaleX=1.2;
	}
	public function setSelectedDate():void{
		application.dcc.dc.selectedDate=new Date(new Date().getFullYear(),new Date().getMonth(),9);
	}
	public function setX():void{
		application.dc_position.x=200;
	}
	public function setY():void{
		application.dc_position.y=200;
	}
      	public function setWidth():void{
		application.dcc.dc.width=200;
      	}
      	public function setHeight():void{
		application.dcc.dc.height=200;
      	}
      	public function setPercentWidth():void{
		application.dcc.dc.percentWidth=200;
      	}
      	public function setPercentHeight():void{
		application.dcc.dc.percentHeight=200;
      	}
      	public function setMaxHeight():void{
      		application.dcc.dc.maxHeight=50;
      	}
      	public function setMaxWidth():void{
      		application.dcc.dc.maxWidth=50;
      	}
      	public function setMinYear():void{
      		application.dcc.dc.minYear=1900;
      	}
      	public function setMaxYear():void{
      		application.dcc.dc.maxYear=2100;
      	}
	public function setToolTip():void
	{
		application.dcc.dc.toolTip="My DateChooser";
	}
	]]>
