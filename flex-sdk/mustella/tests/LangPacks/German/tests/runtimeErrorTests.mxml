    }

    private function triggerIncorrectAdditionRTE():void{
    	var c:ListCollectionView = new ListCollectionView();
    	var aL:ArrayList = new ArrayList();
    	c.sort = new Sort();
    	c.sort.unique = true;
    	aL.addItem(new Object());
    	c.list = aL;
    	aL.addItem(new Object());
    }

    private function triggerItemNotFoundRTE():void{
